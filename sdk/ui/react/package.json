{
  "name": "@boxyhq/react-ui",
  "description": "React UI components from BoxyHQ",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/@boxyhq/react-sso.umd.cjs",
  "module": "./dist/@boxyhq/react-sso.js",
  "exports": {
    ".": {
      "import": "./dist/@boxyhq/react-sso.js",
      "require": "./dist/@boxyhq/react-sso.umd.cjs"
    }
  },
  "scripts": {
    "dev": "SHOW_ALL_COMPONENT_DEMOS=true vite serve docs",
    "build-docs": "rimraf docs/dist && vite build docs && serve -s docs/dist",
    "ssr-docs": "rimraf docs/dist && vite-pages ssr docs && serve docs/dist",
    "build-lib": "vite build"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "5.3.4"
  },
  "devDependencies": {
    "@mdx-js/mdx": "2.1.5",
    "@mdx-js/react": "2.1.5",
    "@mdx-js/rollup": "2.1.5",
    "@rollup/plugin-typescript": "10.0.1",
    "@types/node": "18.11.14",
    "@types/react": "18.0.26",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react": "2.2.0",
    "rimraf": "3.0.2",
    "serve": "14.1.2",
    "vite": "3.2.4",
    "vite-pages-theme-doc": "3.4.0",
    "vite-plugin-mdx": "3.5.11",
    "vite-plugin-react-pages": "3.3.1"
  }
}
