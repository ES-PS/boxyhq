    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.1",
    "rimraf": "4.1.2",
    "serve": "14.2.0",
    "vite": "4.1.1",
    "vite-pages-theme-doc": "4.0.1",
